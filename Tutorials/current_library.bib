keywords = {Biodiversity,Commercial harvest,Exploitation rate trends,Mixed stock fisheries,Pacific salmon,Tradeoffs},
pages = {25--32},
publisher = {Elsevier Ltd},
title = {{The managed decline of British Columbia's commercial salmon fishery}},
url = {https://doi.org/10.1016/j.marpol.2018.12.014},
