issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {7},
pages = {1057--1067},
title = {{Consistent patterns of maturity and density-dependent growth among populations of walleye (Sander vitreus): application of the growing degree-day metric}},
volume = {67},
year = {2010}
}
