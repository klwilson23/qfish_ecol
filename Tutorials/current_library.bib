isbn = {9783319104423},
issn = {16113349},
journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
keywords = {Shape analysis,hierarchical Bayes,sampling in shape space},
number = {PART 3},
pages = {9--16},
title = {{Hierarchical Bayesian modeling, estimation, and sampling for multigroup shape analysis}},
volume = {8675 LNCS},
year = {2014}
