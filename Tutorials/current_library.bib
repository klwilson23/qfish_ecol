pages = {610--620},
title = {{Replicated sampling increases efficiency in monitoring biological populations Replicated sampling increases efficiency in monitoring biological populations}},
url = {http://www.jstor.org/stable/25661086%0Ahttp://www.jstor.org/stable/25661086?seq=1&cid=pdf-reference#references_tab_contents%0Ahttp://about.jstor.org/terms},
volume = {91},
