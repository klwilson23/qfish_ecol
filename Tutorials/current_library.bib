doi = {10.1111/gcb.14676},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Li et al. - 2019 - Subregional differences in groundfish distributional responses to anomalous ocean bottom temperatures in the northeas.pdf:pdf},
issn = {13652486},
journal = {Global Change Biology},
keywords = {centroids,climate change,depth,global warming,marine heat wave,poleward,subregion,survey variability,the center of gravity,thermal ecoregion},
number = {8},
pages = {2560--2575},
pmid = {31058395},
title = {{Subregional differences in groundfish distributional responses to anomalous ocean bottom temperatures in the northeast Pacific}},
volume = {25},
year = {2019}
}
