file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sanchirico, Wilen - 2005 - Optimal spatial management of renewable resources Matching policy scope to ecosystem scale.pdf:pdf},
issn = {00950696},
journal = {Journal of Environmental Economics and Management},
keywords = {Fisheries management,Metapopulation,Price instruments,Spatial management,Zoning},
