@article{Manhard2017,
abstract = {Accumulating evidence has indicated that many fish populations are responding to climate change through shifts in migration time, but genetic data identifying the role of evolution in these shifts are rare. One of the first demonstrations of evolution of migration time was produced by monitoring allozyme alleles that were experimentally manipulated to genetically mark late-migrating pink salmon (Oncorhynchus gorbuscha). Here, we extend that research by using observations of the marker alleles in fry to demonstrate that these changes in migration time were caused by directional selection against the late-migrating phenotype during the oceanic phase of the salmonid life cycle. The selective event, which appeared to be driven by early vernal warming of the nearshore marine environment and consequent decreased survival of late-migrating fry relative to earlymigrating fry, decreased the late-migrating phenotype from more than 50% to approximately 10% of the total fry abundance in only one generation. These demographic changes have persisted over the subsequent 13 generations and suggest that a larger trend toward earlier migration time in this population may reflect adaptation to warming sea-surface temperatures.},
author = {Manhard, Christopher V. and Joyce, John E. and Gharrett, Anthony J.},
doi = {10.1139/CJFAS-2017-0028},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Manhard, Joyce, Gharrett - 2017 - Evolution of phenology in a Salmonid population A potential adaptive response to climate change.pdf:pdf},
issn = {12057533},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {10},
pages = {1519--1527},
publisher = {Canadian Science Publishing},
title = {{Evolution of phenology in a Salmonid population: A potential adaptive response to climate change}},
volume = {74},
year = {2017}
}
@article{Peterson2008,
abstract = {Subspecies of inland cutthroat trout Oncorhynchus clarkii occupy a fraction of their historic \nranges, and displacement by nonnative brook trout Salvelinus fontinalis is among the greatest threats to \nexisting populations in small, headwater streams. Electrofishing is often used to suppress brook trout and \nenhance cutthroat trout populations, but these efforts are labor intensive and costly. To help managers more \neffectively plan and implement brook trout control programs, we used survival estimates from a field \nexperiment to construct matrix population models for both species and linked the models by making the vital \nrates of young cutthroat trout a function of brook trout density to represent the effect of invasion. We then \nexplored the response of cutthroat trout populations (growth rate [k] and probability of persistence for 50 \nyears) to brook trout suppression across various levels of electrofishing frequency, sampling intensity, capture \nefficiency, and brook trout immigration rate. Cutthroat trout (k¼1.10) and brook trout (k¼1.20) both had \nmoderate to high values of k when biotic interactions were weak or absent, but the cutthroat trout k declined \nmarkedly (k ¼ 0.61) when biotic interactions with brook trout were strong. Model results supported the \nhypothesis that rapid displacement of cutthroat trout is facilitated by the brook trout's inherent demographic \nadvantage and biotic effects on young cutthroat trout. Where annual brook trout suppression was not possible, \nthe temporal distribution of suppression events within a systematic control design strongly influenced the \nresponse of cutthroat trout. The absolute response and cost effectiveness of suppression efforts were \ninfluenced particularly by the brook trout immigration rate and also by removal efficiency. Multiple \nconsecutive years of suppression (!3 years) benefited cutthroat trout, but to be effective such suppression \nshould not be interrupted for more than two consecutive years once initiated. Eradication would be ideal, but \nthe maintenance control of brook trout and the management of cutthroat trout in sympatry with brook trout \nwill probably remain among the viable management options for the foreseeable future.},
author = {Peterson, Douglas P. and Fausch, Kurt D. and Watmough, James and Cunjak, Richard A.},
doi = {10.1577/M07-174.1},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Peterson et al. - 2008 - When Eradication is not an Option Modeling Strategies for Electrofishing Suppression of Nonnative Brook Trout t.pdf:pdf},
issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
number = {6},
pages = {1847--1867},
title = {{When Eradication is not an Option: Modeling Strategies for Electrofishing Suppression of Nonnative Brook Trout to Foster Persistence of Sympatric Native Cutthroat Trout in Small Streams}},
url = {http://doi.wiley.com/10.1577/M07-174.1},
volume = {28},
year = {2008}
