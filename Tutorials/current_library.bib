doi = {10.1111/j.1600-0633.1997.tb00137.x},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dewey, Richardson, Zigler - 1997 - Patterns of foraging and distribution of bluegill sunfish in a Mississippi River backwater Influence.pdf:pdf},
