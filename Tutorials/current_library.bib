issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
number = {4},
pages = {837--851},
title = {{Determination of Salmonid Smolt Yield with Rotary-Screw Traps in the Situk River, Alaska, to Predict Effects of Glacial Flooding}},
