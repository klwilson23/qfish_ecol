keywords = {average weight,catch rates,fishing success,fishing tournaments,submersed macrophytes},
pages = {33--38},
title = {{Relations between submersed macrophyte abundance and largemouth bass tournament sucess on two Tennessee River impoundments}},
volume = {34},
year = {1996}
