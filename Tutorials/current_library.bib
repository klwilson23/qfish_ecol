journal = {Source: Oecologia Oecologia},
keywords = {Fish predation @BULLET,Macroinvertebrates @BULLET,Macrophyte structure @BULLET,Periphyton @BULLET,Trophic interactions},
number = {150},
pages = {141--154},
title = {{International Association for Ecology Habitat Structural Complexity Mediates Food Web Dynamics in a Freshwater Macrophyte Habitat structural complexity mediates food web dynamics in a freshwater macrophyte community}},
