file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Burt et al. - 2020 - Enabling coexistence Navigating predator-induced regime shifts in human-ocean systems.pdf:pdf},
issn = {25758314},
journal = {People and Nature},
keywords = {Indigenous knowledge,adaptive governance,collaborative management,kelp forests,keystone predator,predator-recovery,social-ecological systems,trophic cascade},
month = {sep},
number = {3},
pages = {557--574},
publisher = {Blackwell Publishing Ltd},
