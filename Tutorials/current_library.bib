journal = {Transactions of the American Fisheries Society},
number = {6},
pages = {1012--1020},
title = {{ Sampling Fishes in Vegetated Habitats: Effects of Habitat Structure on Sampling Characteristics of the 1-m 2 Throw Trap }},
