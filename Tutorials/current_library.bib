file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bennett et al. - 2009 - Optimisation of underwater visual census and controlled angling methods for monitoring subtidal temperate reef f.pdf:pdf},
issn = {1814232X},
journal = {African Journal of Marine Science},
keywords = {Catch per unit effort,Chrysoblephus laticeps,Fisheries management,Instantaneous area point count,Linefish,Marine protected area,Strip transect,Variability},
number = {3},
