issn = {00218790},
journal = {Journal of Animal Ecology},
keywords = {Activity,Consumption,Reproductive investment,Sexual dimorphism,Walleye,Yellow perch},
number = {5},
pages = {916--926},
title = {{Lazy males? Bioenergetic differences in energy acquisition and metabolism help to explain sexual size dimorphism in percids}},
