issn = {10959238},
journal = {Journal of Archaeological Science},
keywords = {Circuitscape,GIS,Geospatial technologies,Great Lakes,Late prehistory,Movement,Past Landscapes},
number = {10},
pages = {2523--2535},
publisher = {Elsevier Ltd},
title = {{Multiple pathways across past landscapes: Circuit theory as a complementary geospatial method to least cost path for modeling past movement}},
url = {http://dx.doi.org/10.1016/j.jas.2011.03.024},
volume = {38},
