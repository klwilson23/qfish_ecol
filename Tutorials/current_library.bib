@article{Chen2015,
abstract = {AbstractBayesian alternatives to frequentist propensity score approaches have recently been proposed. However, few studies have investigated their covariate balancing properties. This article compares a recently developed two-step Bayesian propensity score approach to the frequentist approach with respect to covariate balance. The effects of different priors on covariate balance are evaluated and the differences between frequentist and Bayesian covariate balance are discussed. Results of the case study reveal that both the Bayesian and frequentist propensity score approaches achieve good covariate balance. The frequentist propensity score approach performs slightly better on covariate balance for stratification and weighting methods, whereas the two-step Bayesian approach offers slightly better covariate balance in the optimal full matching method. Results of a comprehensive simulation study reveal that accuracy and precision of prior information on propensity score model parameters do not greatly influence balance performance. Results of the simulation study also show that overall, the optimal full matching method provides the best covariate balance and treatment effect estimates compared to the stratification and weighting methods. A unique feature of covariate balance within Bayesian propensity score analysis is that we can obtain a distribution of balance indices in addition to the point estimates so that the variation in balance indices can be naturally captured to assist in covariate balance checking.},
author = {Chen, Jianshen and Kaplan, David},
doi = {10.1080/19345747.2014.911396},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen, Kaplan - 2015 - Covariate Balance in Bayesian Propensity Score Approaches for Observational Studies.pdf:pdf},
issn = {19345739},
journal = {Journal of Research on Educational Effectiveness},
keywords = {Bayesian statistics,Propensity scores,covariate balance},
number = {2},
pages = {280--302},
title = {{Covariate Balance in Bayesian Propensity Score Approaches for Observational Studies}},
volume = {8},
year = {2015}
}
@article{Post2015,
abstract = {Summary Canada has vast quantities of fresh water, covering c. 9% of its land mass, more than any other country in the world. Southern Canada, south of 60°N, has a relatively depauperate freshwater fish fauna with 147 species exclusively fresh water, 48 species that are anadromous and catadromous or have populations in both freshwater and marine systems and 16 introduced species. These fishes are distributed across nine ecoregions as defined by a combination of faunal similarity and major watersheds. Diversity varies substantially across ecoregions due to variability in glacial history, topography, climate and spatial patterns and abundance of lakes, rivers and wetlands. Recreational fisheries are dominant in these fresh waters where c. 3.71???106 anglers contribute Canadian $8.3 billion to the economy. The most popular recreational fish species by catch are Sander vitreus (26%), salmonids (22%), Perca flavescens (17%), basses (12%) and Esox lucius (12%), and 11% other species. Canada has some of the best recreational fisheries in the world, yet there is evidence of local overfishing and collapse, in particular in fisheries closest to large urban areas where they are often maintained through stocking. If the combination of human population growth and angling participation rate continue as they have recently, we can expect to see continuing erosion of fishing quality, particularly within the shadows of large cities. Commercial fishing in Canadian fresh waters is relatively modest in landings and value in contrast to Canadian marine commercial fisheries accounting for c. 3% of the landed volume and 4% of the total landed value (Canadian $68???106) of all Canadian commercial fisheries. Sander vitreus, Coregonus spp., P. flavescens and E. lucius are the primary targets for the Canadian freshwater commercial fishery. Effective management of Canada's fresh water fisheries involves managing harvest, habitat and fish stocks. The richness and diversity of fresh waters and their associated fisheries in Canada provide substantial challenges to sustainable management. Approaches involving landscape-scale adaptive management in which fishes, fishers and managers are considered dynamic players in complex resource management system hold promise in sustaining high-quality freshwater fisheries and their substantial social, cultural and economic value to Canada.},
author = {Post, John R. and Mandrak, Nicholas E. and Burridge, Mary},
doi = {10.1002/9781118394380.ch11},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Post, Mandrak, Burridge - 2015 - Canadian freshwater fishes, fisheries and their management, south of 60°N.pdf:pdf},
isbn = {9781118394380},
journal = {Freshwater Fisheries Ecology},
keywords = {Biodiversity,Commercial fisheries,Ecoregions,Recreational fisheries},
pages = {151--165},
title = {{Canadian freshwater fishes, fisheries and their management, south of 60°N}},
year = {2015}
}
@article{Higgins2015,
abstract = {Understanding growth in living organisms is central to the study of\nindividuals, populations and communities. In fisheries, in particular,\ngrowth modelling is fundamental to the management of fish stocks that\ncan constitute important economic resources. The scientific literature\nis abundant in studies attempting to define the growth trajectory of\ndifferent fish species but often little consideration is given to the\necological components of the fish's life history. This review examines\nontogeny of fishes with complex life histories, in particular how\ndifferent phases can effect growth and the interpretation of growth\nmodels. Using the blackspot seabream (Pagellus bogaraveo) as an example,\nwe explore alternative approaches to understanding growth in a species\nthat undergoes both sex reallocation and ontogenetic vertical migration.\nMore flexible models are required in order to accurately represent\nsignificant lifetime events, specifically accounting for periodical\nchanges in the growth trajectory. Finally, model evaluation and\ninterpretation need to be adequate to the task, particularly when\noutcomes are used to support resource management, in order to address\nconcerns regarding conservation, sustainability, and successful\nmanagement in the long term.},
author = {Higgins, R. M. and Diogo, H. and Isidro, E. J.},
doi = {10.1007/s11160-015-9388-8},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Higgins, Diogo, Isidro - 2015 - Modelling growth in fish with complex life histories.pdf:pdf},
issn = {09603166},
journal = {Reviews in Fish Biology and Fisheries},
keywords = {Growth,Habitat shift,Hermaphrodite,Modelling,Ontogenetic migration},
number = {3},
pages = {449--462},
publisher = {Springer International Publishing},
title = {{Modelling growth in fish with complex life histories}},
volume = {25},
year = {2015}
}
