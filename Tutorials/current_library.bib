title = {{Rates of straying by hatchery-produced Pacific Salmon (Oncorhynchus spp.) and steelhead (Oncorhynchus mykiss) differ among species, life history types, and populations}},
url = {https://cdnsciencepub.com/doi/abs/10.1139/cjfas-2012-0536},
