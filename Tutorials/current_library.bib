journal = {Journal of Mammalogy},
keywords = {age-specific survival,capture,cmr,cormack-jolly-seber model,environmental covariates,mark,marmota flaviventris,population dynamics,pradel,recapture,s reverse-time model,spatial,spatiotemporal population dynamics,temporal variation,variation,yellow-bellied marmot},
