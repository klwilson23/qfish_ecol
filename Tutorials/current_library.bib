issn = {15488667},
journal = {Journal of Aquatic Animal Health},
number = {1},
pages = {43--50},
title = {{Susceptibility of three species of anadromous salmonids to experimentally induced infections with myxobolus cerebralis, the causative agent of whirling disease}},
volume = {13},
