pages = {395--409},
publisher = {Springer},
title = {{Review of the effects of in-stream pipeline crossing construction on aquatic ecosystems and examination of Canadian methodologies for impact assessment}},
url = {https://link-springer-com.proxy.lib.sfu.ca/article/10.1007/s10661-006-9542-9},
