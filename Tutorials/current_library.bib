file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gresh, Lichatowich, Schoonmaker - 2000 - An Estimation of Historic and Current Levels of Salmon Production in the Northeast Pacific Ecos.pdf:pdf},
