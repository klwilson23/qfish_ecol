file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Brown, Maceina - 2002 - The Influence of Disparate Levels of Submersed Aquatic Vegetation on Largemouth Bass Population Characteristics.pdf:pdf},
isbn = {0146-6623},
issn = {01466623},
journal = {J. Aquat. Plant Manage},
keywords = {Hydrilla verti-cillata,Micropterus salmoides,growth,recruitment,reproduction,survival},
number = {40},
pages = {28--35},
title = {{The Influence of Disparate Levels of Submersed Aquatic Vegetation on Largemouth Bass Population Characteristics in a Georgia Reservoir}},
