keywords = {biotic velocity,climate change,context dependence,dissolved oxygen,groundfish,spatio,spatio-temporal distribution models,temporal distribution models},
month = {jan},
number = {1},
pages = {239--255},
publisher = {John Wiley & Sons, Ltd},
title = {{Contrasting climate velocity impacts in warm and cool locations show that effects of marine warming are worse in already warmer temperate waters}},
