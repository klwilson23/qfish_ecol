@article{Sinnatamby2018,
abstract = {Using individual tags combined with a fish fence operated at the mouth of Smith-Dorrien Creek, the primary spawning habitat for Lower Kananaskis Lake bull trout (Salvelinus confluentus), we approximated a complete census of the spawning population from 1996 to 2000 to assess whether timing of upstream and downstream spawning migrations varied with extrinsic and intrinsic factors. The timing of both upstream and downstream migrations varied with sex, previous spawning experience, density, and temperature. Inferred spawning duration based on the predicted upstream and downstream migration dates indicated that experienced female spawners spent the least amount of time upstream and first-time spawners spent the most time upstream. No consistent differences in upstream migration timing were observed between non-repetitive and repetitive spawners. We suggest that variations in spawning migration timing observed in Lower Kananaskis Lake may be linked to environmental factors that influence upstream swimming ability and acquisition and expenditure of energy with respect to reproduction.},
author = {Sinnatamby, R. Niloshini and Pinto, Madalena C. and Johnston, Fiona D. and Paul, Andrew J. and Mushens, Craig J. and Stelfox, Jim D. and Ward, Hillary G.M. and Post, John R.},
