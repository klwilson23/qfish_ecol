doi = {10.1080/23308249.2019.1583166},
issn = {2330-8249},
journal = {Reviews in Fisheries Science & Aquaculture},
keywords = {Angler behaviors,meta-analysis,revealed preference,stated preference,utility,valuation},
month = {mar},
number = {3},
pages = {1--26},
