publisher = {John Wiley & Sons, Ltd},
title = {{Insights on the concept of indicator populations derived from parentage-based tagging in a large-scale coho salmon application in British Columbia, Canada}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/ece3.6383 https://onlinelibrary.wiley.com/doi/abs/10.1002/ece3.6383 https://onlinelibrary.wiley.com/doi/10.1002/ece3.6383},
