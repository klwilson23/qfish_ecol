issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Australia,Energy flow,Fish,Food web,Seagrass,Stable isotope analysis},
pages = {49--61},
title = {{Trophic dynamics in a relatively pristine subtropical fringing mangrove community}},
volume = {428},
year = {2011}
}
