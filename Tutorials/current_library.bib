doi = {10.1111/j.1365-2664.2011.02062.x},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Moilanen - 2011 - On the limitations of graph-theoretic connectivity in spatial ecology and conservation.pdf:pdf},
