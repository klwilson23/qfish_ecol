journal = {Oikos},
keywords = {Aves,Behavior,Birds,Chordata,Chordates,Ecology: Environmental Sciences,Falconiformes: Animals,Florida: USA,Nearctic region,Nonhuman,North America,Rostrhamus sociabilis: snail kite [Falconiformes],Vertebrata,[07002] Behavioral biology - General and comparati,[07003] Behavioral biology - Animal behavior,[07502] Ecology: environmental biology - General a,[07508] Ecology: environmental biology - Animal,[85534] Falconiformes,food abundance,food availability,food scarcity,movement probabilities,proximate foraging benefits,radio transmitter: equipment,water level},
