keywords = {climate,clonal reproduction,distribution,latitude,local adaptation,plasticity,potamogeton pectinatus,reciprocal transplant,shoot morphology,survival,vegetative growth},
number = {9},
pages = {2454--2461},
title = {{Plant Performance Across Latitude: the Role of Plasticity and Local Adaptation in an Aquatic Plant}},
