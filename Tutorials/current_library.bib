month = {jul},
number = {2},
pages = {189--198},
publisher = {Elsevier BV},
title = {{The re-expansion and improving status of the silver-spotted skipper butterfly (Hesperia comma) in Britain: A metapopulation success story}},
volume = {124},
year = {2005}
}
