issn = {17083087},
journal = {Ecology and Society},
keywords = {Cumulative effects,Forest ecosystems,Governance,Scenario models,Social indicators},
month = {jun},
number = {2},
publisher = {Resilience Alliance},
title = {{Cumulative effects assessment: Linking social, ecological, and governance dimensions}},
url = {http://dx.doi.org/10.5751/ES-04597-170222},
volume = {17},
