@incollection{Tinker2015a,
abstract = {Quantitative models have been used extensively to solve complex problems in sea otter conservation. Models represent powerful tools for simplifying and integrating data across a wide range of topics, from behavior and physiology to complex population dynamics, genetics, disease ecology, food-web interactions, and physical/biological oceanography. When based on solid data and sound assumptions, models allow scientists to test hypotheses, elucidate the underlying mechanisms of a particular problem, and make sound conservation decisions. Here I discuss several branches of conservation science in which sea otter researchers have used mathematical models to aid in their research, including behavior, population dynamics, community interactions, and applied conservation questions. For each subject area, I explain some of the key research questions, provide examples of how models have been used to help answer those questions, and highlight promising areas for future and ongoing work in sea otter conservation. Finally I explore some of the general lessons that can be drawn from these examples, focusing on points that may be useful for wildlife conservation more broadly.},
address = {London},
author = {Tinker, M. Tim},
booktitle = {Sea Otter Conservation},
doi = {10.1016/B978-0-12-801402-8.00010-X},
editor = {Bodkin, J.L. and Vanblaricom, G. R. and Larson, S.},
isbn = {9780128014028},
pages = {257--300},
publisher = {Academic Press},
title = {{The Use of Quantitative Models in Sea Otter Conservation}},
url = {https://www.sciencedirect.com/science/article/pii/B978012801402800010X},
year = {2015}
}
@article{Fletcher2015a,
abstract = {See, stats, and : http:// www. researchgate. net/ publication/ 281442455 Affinity impacts geographical ARTICLE : BIOLOGICAL Impact : 5 . 05 : 10 . 1098 / rspb . 2015 . 1545 READS 39 6 , INCLUDING : Robert University 72 , 726 SEE Brian University 10 SEE James . Austin University 57 SEE Wiley University 75 , 060 SEE Available : Robert Retrieved : 30 rspb . royalsocietypublishing . org Research Cite this article : Fletcher Jr RJ , Robertson EP , Wilcox RC , Reichert BE , Austin JD , Kitchens WM . 2015 Affinity for natal environments by dispersers impacts reproduction and explains geographical structure of a highly mobile bird . Understanding dispersal and habitat selection behaviours is central to many problems in ecology , evolution and conservation . One factor often hypo - thesized to influence habitat selection by dispersers is the natal environment experienced by juveniles . Nonetheless , evidence for the effect of natal environ - ment on dispersing , wild vertebrates remains limited . Using 18 years of nesting and mark – resight data across an entire North American geographical range of an endangered bird , the snail kite (Rostrhamus sociabilis) , we tested for natal effects on breeding - site selection by dispersers and its consequences for repro - ductive success and population structure . Dispersing snail kites were more likely to nest in wetlands of the same habitat type (lacustrine or palustrine) as their natal wetland , independent of dispersal distance , but this preference declined with age and if individuals were born during droughts . Importantly , dispersing kites that bred in natal - like habitats had lower nest success and pro - ductivity than kites that did not . These behaviours help explain recently described population connectivity and spatial structure across their geographi - cal range and reveal that assortative breeding is occurring , where birds are more likely to breed with individuals born in the same wetland type as their natal habitat . Natal environments can thus have long - term and large - scale effects on populations in nature , even in highly mobile animals .},
