journal = {Marine Ecology Progress Series},
keywords = {Clupea pallasii,Eelgrass,Egg loss,Forage fish,Habitat limitation,Habitat limitation {\textperiodcentered} Clupea pallasii,Herring,Nearshore habitat,Puget Sound},
month = {nov},
