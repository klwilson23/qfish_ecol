file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gardner, W., Mulvey, E. P., & Shaw - 1995 - Regression analyses of counts and rates Poisson, overdispersed Poisson, and negative binomia.pdf:pdf},
isbn = {084127438X},
