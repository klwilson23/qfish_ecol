month = {oct},
number = {5},
pages = {1052--1070},
publisher = {John Wiley & Sons, Ltd},
title = {{Protecting our coast for everyone's future: Indigenous and scientific knowledge support marine spatial protections proposed by Central Coast First Nations in Pacific Canada}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/pan3.10380 https://onlinelibrary.wiley.com/doi/abs/10.1002/pan3.10380 https://besjournals.onlinelibrary.wiley.com/doi/10.1002/pan3.10380},
