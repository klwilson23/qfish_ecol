pmid = {23322735},
publisher = {National Academy of Sciences},
title = {{Frequency and intensity of productivity regime shifts in marine fish stocks}},
url = {www.pnas.org/cgi/doi/10.1073/pnas.1214879110},
volume = {110},
year = {2013}
