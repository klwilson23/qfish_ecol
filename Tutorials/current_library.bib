issn = {1461023X},
journal = {Ecology Letters},
keywords = {Agri-environment scheme,Crop diversity,Field margins,Field size,Habitat fragmentation,Intermediate heterogeneity hypothesis,Landscape complementation,Landscape composition,Landscape configuration,Spatial heterogeneity},
