@article{Kaller2011,
abstract = {Low levels of dissolved oxygen (DO) are potential stressors of fishes\nand invertebrates in freshwater and marine systems. Because of the\nincredibly nutrient-rich and warm nature of the Atchafalaya River Basin\n(ARB), microbial decomposition in backwater areas with limited flow\noften results in potentially stressful, if not lethal, DO levels during\nand after the annual flood pulse. As water temperatures increase and\nflood levels fall, DO levels often stratify, restricting fish habitat.\nIn order to better guide development of water management projects, we\ninvestigated the relationship between DO and DO stratification (DOD;\nsurface DO - bottom DO) and a suite of environmental variables\npotentially linked to DO and DOD. Based on continuous monitoring data\ncollected from 2000 to 2007, we used a variable selection procedure to\nconstruct statistical generalized linear models to describe the\nrelationship between DO levels and DOD and several potentially important\npredictor variables at three spatial scales: the entire sampled area of\nthe ARB; individual water management units (WMUs) or subunits (WMSs);\nand individual sampling sites. We conducted these analyses using models\nthat assume the normal distribution and compared them to models that\nincorporate alternate error terms based on other error term\ndistributions. Goodness of fit statistics suggested that our analyses\nbest described the relationship between DO and structuring variables at\nthe WMU/WMS scale. At this scale, water temperature, river stage height,\ncurrent velocity, and to a lesser extent, water depth appeared to be the\nmost important measured variables that influence DO levels. We suggest\nthat management decisions at the WMU and WMS scales will offer the\ngreatest potential benefits for improvement of DO conditions in the ARB.},
author = {Kaller, M. D. and Kelso, W. E. and Halloran, B. T. and Rutherford, D. A.},
doi = {10.1007/s10750-010-0470-x},
file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kaller et al. - 2011 - Effects of spatial scale on assessment of dissolved oxygen dynamics in the Atchafalaya river basin, Louisiana.pdf:pdf},
issn = {00188158},
journal = {Hydrobiologia},
keywords = {Atchafalaya River,Bottomland hardwood forest,Dissolved oxygen,Water management},
number = {1},
pages = {7--15},
title = {{Effects of spatial scale on assessment of dissolved oxygen dynamics in the Atchafalaya river basin, Louisiana}},
volume = {658},
year = {2011}
}
@article{stock_woods_2021,
abstract = {The rapid changes observed in many marine ecosystems that support fisheries pose a challenge to stock assessment and management predicated on time-invariant productivity and considering species in isolation. In single-species assessments, two main approaches have been used to account for productivity changes: allowing biological parameters to vary stochastically over time (empirical), or explicitly linking population processes such as recruitment (R) or natural mortality (M) to environmental covariates (mechanistic). Here, we describe the Woods Hole Assessment Model (WHAM) framework and software package, which combines these two ap­ proaches. WHAM can estimate time- and age-varying random effects on annual transitions in numbers at age (NAA), M, and selectivity, as well as fit environmental time-series with process and observation errors, missing data, and nonlinear links to R and M. WHAM can also be configured as a traditional statistical catch-at-age (SCAA) model in order to easily bridge from status quo models and test them against models with state-space and environmental effects, all within a single framework.},
author = {Stock, Brian C and Miller, Timothy J},
doi = {10.1016/j.fishres.2021.105967},
issn = {01657836},
journal = {Fisheries Research},
month = {aug},
pages = {105967},
shorttitle = {The {Woods} {Hole} {Assessment} {Model} ({WHAM})},
title = {{The {Woods} {Hole} {Assessment} {Model} ({WHAM}): {A} general state-space assessment framework that incorporates time- and age-varying processes via random effects and links to environmental covariates}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0165783621000953},
volume = {240},
year = {2021}
}
