keywords = {Bluegill,Eurasian milfoil,Largemouth bass,Macrophyte management,Simulation model},
number = {94},
pages = {139--156},
title = {{A model of bluegill-largemouth bass interactions in relation to aquatic vegetation and its management}},
url = {http://ac.els-cdn.com/S0304380096000099/1-s2.0-S0304380096000099-main.pdf?_tid=af26f092-5112-11e7-90cc-00000aacb35e&acdnat=1497452814_859e72b503cb4e2333b1a75d533199ef},
