file = {:C\:/Users/kylel/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kittinger et al. - 2013 - Emerging frontiers in social-ecological systems research for sustainability of small-scale fisheries.pdf:pdf},
issn = {18773435},
