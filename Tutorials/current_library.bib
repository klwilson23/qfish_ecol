journal = {Conservation Biology},
keywords = {Artisanal fishery,Coral reef,Kenya,Livelihood portfolios,Poverty trap,Socioeconomics},
number = {1},
pages = {124--130},
title = {{Socioeconomic factors that affect artisanal fishers' readiness to exit a declining fishery}},
volume = {23},
year = {2009}
}
